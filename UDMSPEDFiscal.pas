{procedure TDMSPEDFiscal.prc_Abrir_NotaFiscal(Bloco : String ; DataIni, DataFin : TDateTime);
begin
  cdsNotaFiscal.Close;
  sdsNotaFiscal.CommandText := ctNotaFiscal
                             + ' WHERE (((N.DTEMISSAO BETWEEN ' + QuotedStr(FormatDateTime('MM/DD/YYYY',DataIni))
                             + '    AND  ' + QuotedStr(FormatDateTime('MM/DD/YYYY',DataFin.date))


SELECT N.*
FROM NOTAFISCAL N
WHERE (((N.dtemissao between :DT_INICIAL AND :DT_FINAL) AND (N.tipo_reg = 'NTS'))
       or ((N.dtsaidaentrada between :DT_INICIAL AND :DT_FINAL ) AND (N.tipo_reg = 'NTE')))
  AND N.FILIAL = :FILIAL




end;}

